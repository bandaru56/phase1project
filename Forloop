package javaPrograms;

public class Forloop {

	public static void main(String[] args) {
		
		for(int i=1;i<=10;i++)
		{
			System.out.println(i);
		}
		for(int i=100;i>=50;i--)  // print values from 100 to 50
		{
			System.out.println(i);
		}
		
		System.out.println("Im out of the loop");
		
		 i++; 

	}

}
